hipify-clang gaussian.cu --cuda-path=/usr/local/cuda \
    -I /usr/local/cuda/samples/common/inc \
    -I../.. -o gaussian.cc
g++ -std=c++17 -o gaussian gaussian.cc -lpthread -ltbb -fpermissive -I/nethome/rhan38/Intel_PPoPP_evaluate/SC_evaluate/HIP-CPU/include
./gaussian -f ../../data/gaussian/matrix4.txt